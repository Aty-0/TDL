    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Developer.Desktop/bin/Debug/net5.0/Developer.dll",
            //"program": "${workspaceFolder}/Developer.Game.Tests/bin/Debug/net5.0/Developer.Game.Tests.dll",
            "args": ["-dev", "-autoloadsave"],
            "cwd": "${workspaceFolder}/Developer.Desktop",
            //"cwd": "${workspaceFolder}/Developer.Game.Tests",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}